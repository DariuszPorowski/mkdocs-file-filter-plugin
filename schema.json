{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "https://json-schema.org/draft/2019-09/schema",
	"title": "mkdocs-file-filter-plugin schema",
	"oneOf": [
		{
			"markdownDescription": "https://dariusz.porowski.dev/mkdocs-file-filter-plugin/#readme",
			"enum": [
				"file-filter"
			]
		},
		{
			"type": "object",
			"properties": {
				"file-filter": {
					"markdownDescription": "https://dariusz.porowski.dev/mkdocs-file-filter-plugin/#configuring",
					"type": "object",
					"default": null,
					"properties": {
						"config": {
							"title": "External config file path",
							"markdownDescription": "https://mkdocstrings.github.io/handlers/overview/",
							"type": "string",
							"default": null
						}
					},
					"anyOf": [
						{
							"$ref": "schema-config.json"
						}
					],
					"unevaluatedProperties": true,
					"additionalProperties": true
				}
			},
			"additionalProperties": false
		}
	]
}
